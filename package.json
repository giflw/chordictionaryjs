{
	"name": "chordictionary",
	"version": "0.2.0-SNAPSHOT",
	"description": "A Javascript library for dynamic chord recognition, generation and graphic representation for any fretted instrument.",
	"type": "module",
	"files": [
		"dist"
	],
	"module": "./dist/chordictionary.js",
	"main": "./dist/chordictionary.cjs",
	"exports": {
		".": {
			"import": "./dist/chordictionary.js",
			"require": "./dist/chordictionary.cjs"
		}
	},
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"preview": "vite preview",
		"clean": "npx rimraf dist",
		"test": "jasmine"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/giflw/chordictionaryjs.git"
	},
	"keywords": [
		"music",
		"score",
		"tab",
		"chord",
		"note",
		"fret",
		"fretboard",
		"guitar",
		"instrument"
	],
	"author": "Hubert Fauconnier <h.fauconnier@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/giflw/chordictionaryjs/issues"
	},
	"homepage": "https://www.chordictionary.com",
	"dependencies": {},
	"devDependencies": {
		"chai": "^4.3.10",
		"eslint": "^8.53.0",
		"jasmine": "^5.1.0",
		"sass": "^1.69.5",
		"vite": "^4.5.0"
	}
}
